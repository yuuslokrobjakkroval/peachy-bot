# Discord Bot Token (NEVER share your real token)
TOKEN="YOUR_DISCORD_BOT_TOKEN"

# Command Prefix (e.g. !, ?, etc.)
PREFIX="!"

# Discord User IDs of bot owners (comma-separated)
OWNER_IDS=123456789012345678,234567890123456789

# Bank account (main profile or guild wallet)
BANKACCOUNT_ID=123456789012345678

# Discord Application credentials
CLIENT_ID="YOUR_DISCORD_CLIENT_ID"
CLIENT_SECRET="YOUR_DISCORD_CLIENT_SECRET"

# Main guild/server ID for development/testing
GUILD_ID="YOUR_DISCORD_GUILD_ID"

# Set to true in production, false or blank for development
PRODUCTION="false"

# MongoDB connection URI
DATABASE_URL="mongodb://localhost/YOUR_DATABASE_NAME"

# Gemini or Google API key (AI, image gen, etc.)
GEMINI_API_KEY="YOUR_GEMINI_API_KEY"

# Node environment (use 'development' or 'production')
NODE_ENV="development"
