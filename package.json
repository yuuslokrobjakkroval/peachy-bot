{
  "name": "peachy",
  "version": "1.0.2",
  "description": "Peachy is a discord bot made by Yuu",
  "main": "src/index.js",
  "repository": "https://github.com/yuuslokrobjakkroval/peachy-bot.git",
  "author": "KYUU",
  "license": "ISC",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --watch src --ext js,json --exec node src/index.js",
    "restart": "npm run stop && npm run start",
    "stop": "taskkill /F /IM node.exe",
    "format": "prettier --write .",
    "prettier": "prettier --write .",
    "prettier-check": "prettier --check .",
    "debug": "NODE_ENV=development node --inspect src/index.js"
  },
  "keywords": [
    "discord",
    "bot",
    "discordjs",
    "discord-bot",
    "moderation",
    "economy",
    "fun",
    "utility",
    "community",
    "slash-commands",
    "discord-bot-framework"
  ],
  "bugs": "",
  "homepage": "",
  "devDependencies": {
    "@types/signale": "^1.4.6",
    "nodemon": "^3.1.8"
  },
  "dependencies": {
    "@discordjs/builders": "^1.11.2",
    "@hammerhq/localization": "^2.2.0",
    "axios": "^1.10.0",
    "canvafy": "^7.2.1",
    "chance": "^1.1.13",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "mongoose": "^8.16.4",
    "node-cron": "^4.2.1",
    "signale": "^1.4.0"
  },
  "signale": {
    "displayScope": true,
    "displayBadge": true,
    "displayDate": true,
    "displayFilename": true,
    "displayLabel": true,
    "displayTimestamp": true,
    "underlineLabel": true
  }
}
