{
    "name": "peachy",
    "version": "1.0.0",
    "description": "Peachy is a discord bot made by Yuu",
    "main": "src/index.js",
    "repository": "https://github.com/yuuslokrobjakkroval/peachy-bot.git",
    "author": "KYUU",
    "license": "ISC",
    "scripts": {
        "start": "node src/index.js",
        "start:bun": "bun run src/index.js",
        "dev": "nodemon --watch src --ext js,json --exec node src/index.js",
        "dev:bun": "bun --watch src/index.js",
        "restart": "npm run stop && npm run start",
        "restart:bun": "npm run stop && npm run start:bun",
        "stop": "taskkill /F /IM node.exe",
        "stop:bun": "taskkill /F /IM bun.exe",
        "format": "prettier --write .",
        "prettier": "prettier --write .",
        "prettier-check": "prettier --check .",
        "debug": "NODE_ENV=development node --inspect src/index.js",
        "debug:bun": "NODE_ENV=development bun --inspect src/index.js",
        "lint": "biome lint src/",
        "lint:fix": "biome lint src/ --write",
        "lint:check": "biome lint src/ --write --unsafe",
        "format:biome": "biome format src/",
        "format:biome:fix": "biome format src/ --write",
        "check": "biome check src/",
        "check:fix": "biome check src/ --write"
    },
    "keywords": [
        "discord",
        "bot",
        "discordjs",
        "discord-bot",
        "moderation",
        "economy",
        "fun",
        "utility",
        "community",
        "slash-commands",
        "discord-bot-framework"
    ],
    "bugs": "",
    "homepage": "",
    "devDependencies": {
        "@biomejs/biome": "^2.3.8",
        "@types/signale": "^1.4.6",
        "nodemon": "^3.1.8"
    },
    "dependencies": {
        "@discordjs/builders": "^1.11.2",
        "@hammerhq/localization": "^2.2.0",
        "axios": "^1.10.0",
        "canvafy": "^7.2.1",
        "chance": "^1.1.13",
        "discord.js": "^14.23.2",
        "dotenv": "^17.2.0",
        "moment": "^2.30.1",
        "moment-timezone": "^0.6.0",
        "mongoose": "^8.16.4",
        "node-cron": "^4.2.1",
        "prettier": "^3.6.2",
        "signale": "^1.4.0"
    },
    "signale": {
        "displayScope": true,
        "displayBadge": true,
        "displayDate": true,
        "displayFilename": true,
        "displayLabel": true,
        "displayTimestamp": true,
        "underlineLabel": true
    }
}
